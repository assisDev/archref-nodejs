{
    "web": {
      "port": "$vault.port"
    },
    "db": {
      "username": "$vault.dbUsername",
      "password": "$vault.dbPassword",
      "database": "$vault.databaseName",
      "mongodbHost": ["$vault.mongodbHost"],
      "dialect": "$vault.mongodbDialect",
      "logging": "",
      "options": {
        "authSource": "$vault.authSource",
        "replicaSet": "$vault.replicaSet"
      }
    }
  }
  